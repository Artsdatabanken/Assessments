@model NatureTypesListViewModel

<div class="list-search">
    <label asp-for="Name"><b>Søk i @NatureTypesConstants.Title2025</b></label>
    <div class="searchfield">
        <div class="searchfield_and_button">
            <input asp-for="Name" autocomplete="off">
            <button class="primary" type="submit">
                <span class="material-icons">
                    search
                </span>
                Søk
            </button>
        </div>
    </div>
    <small class="search-help-text">
        F.eks. flomskogsmark
    </small>
</div>