@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.SelectedMenuItem = "Naturtyper";
    IgnoreSection("PageMenu");
}

@section Styles
{
    <link rel="stylesheet" href="~/css/graphs.css">
    <link rel="stylesheet" href="~/css/listview.css">
    <link rel="stylesheet" href="~/css/assessment.css">
    <link rel="stylesheet" href="~/css/filter.css">
    <link rel="stylesheet" href="~/css/search.css">
    <link rel="stylesheet" href="~/css/toggleSwitch.css">
    <link rel="stylesheet" href="~/css/tabs.css">
    <link rel="stylesheet" href="~/css/map.css">
    <link rel="stylesheet" href="~/css/categorybar.css">
    @await RenderSectionAsync("Styles", required: false)
}

@section Breadcrumb
{
    @await RenderSectionAsync("Breadcrumb", required: false)
}

@RenderBody()

@section Scripts
{
    @await RenderSectionAsync("Scripts", required: false)
}
